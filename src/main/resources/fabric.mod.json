{
  "schemaVersion": 1,
  "id": "denovo",
  "version": "${version}",

  "name": "De Novo",
  "description": "Make early game BTW more survivable, without killing all challenge",
  "authors": [
    "Zhil",
    "Sockthing"
  ],
  "contact": {
    "homepage": "https://github.com/BTW-Community/De-Novo",
    "sources": "https://github.com/BTW-Community/De-Novo"
  },

  "license": "MIT",
  "icon": "assets/denovo/icon.png",

  "environment": "*",
  "entrypoints": {
    "btw:addon": [
      "btw.community.denovo.DeNovoAddon"
    ],
    "emi": [
